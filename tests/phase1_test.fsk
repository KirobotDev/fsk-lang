let double = x => x * 2;
print "Double 5: " + double(5); 

let add = (a, b) => a + b;
print "Add 3, 4: " + add(3, 4); 

let greet = () => {
    print "Hello from arrow!";
};
greet();

fn multiply(a, b = 2) {
    return a * b;
}
print "Multiply 4 (default 2): " + multiply(4); 
print "Multiply 4, 3: " + multiply(4, 3); 

let [x, y, ...rest] = [1, 2, 3, 4, 5];
print "x: " + x; 
print "y: " + y; 
print "rest: " + rest;
