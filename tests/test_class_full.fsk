class Point {
    fn init(x, y) {
        this.x = x;
        this.y = y;
    }

    fn show() {
        print "Point(" + this.x + ", " + this.y + ")";
    }

    fn add(other) {
        return Point(this.x + other.x, this.y + other.y);
    }
}

let p1 = Point(10, 20);
p1.show();

let p2 = Point(5, 5);
let p3 = p1.add(p2);
p3.show();
