class Dev {
    fn sayHi() {
        print "Hi, I'm " + this.name
    }
}

let dev = Dev()
dev.name = "Sion"
dev.sayHi()

class Counter {
    fn increment() {
        this.count = this.count + 1
    }
}

let c = Counter()
c.count = 0
c.increment()
c.increment()
print "Count is: " + c.count

class User {
    fn init(name, role) {
        this.name = name
        this.role = role
    }

    fn info() {
        print this.name + " (" + this.role + ")"
    }
}

let u = User("Root", "Admin")
u.info()
