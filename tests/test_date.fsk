
print "Testing Date Module...";

let now = Date.now();
print "Current timestamp (now): " + now;

let ts = Date.timestamp();
print "Current timestamp (ts): " + ts;

if (now > 1700000000) {
    print "Timestamp looks valid (recent).";
} else {
    print "Error: Timestamp seems invalid.";
}

let formatted = Date.format(now, "%Y-%m-%d");
print "Formatted Date: " + formatted;

let epoch = Date.format(0, "%Y-%m-%d");
print "Epoch Date (should be 1970-01-01): " + epoch;

if (epoch == "1970-01-01") {
    print "Formatting test passed.";
} else {
    print "Formatting test failed.";
}
