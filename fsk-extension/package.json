{
    "name": "fsk-lang",
    "displayName": "Fsk Language Support (Antigravity Edition)",
    "description": "Syntax highlighting and language support for the Fsk programming language.",
    "version": "1.0.0",
    "publisher": "kirobot",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "fsk",
                "aliases": [
                    "Fsk",
                    "fsk"
                ],
                "extensions": [
                    ".fsk"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fsk",
                "scopeName": "source.fsk",
                "path": "./syntaxes/fsk.tmLanguage.json"
            }
        ]
    }
}