{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Fsk",
    "patterns": [
        {
            "include": "#keywords"
        },
        {
            "include": "#strings"
        },
        {
            "include": "#comments"
        },
        {
            "include": "#numbers"
        }
    ],
    "repository": {
        "keywords": {
            "patterns": [
                {
                    "name": "keyword.control.fsk",
                    "match": "\\b(if|else|while|for|return|try|catch|throw|async|await|import|spawn)\\b"
                },
                {
                    "name": "keyword.declaration.fsk",
                    "match": "\\b(let|const|fn|class|interface)\\b"
                },
                {
                    "name": "constant.language.fsk",
                    "match": "\\b(true|false|nil|this|super)\\b"
                },
                {
                    "name": "support.function.fsk",
                    "match": "\\b(print|sleep|exit|type)\\b"
                },
                {
                    "name": "support.class.fsk",
                    "match": "\\b(FSK|JSON|Math|List)\\b"
                }
            ]
        },
        "strings": {
            "name": "string.quoted.double.fsk",
            "begin": "\"",
            "end": "\"",
            "patterns": [
                {
                    "name": "constant.character.escape.fsk",
                    "match": "\\\\."
                }
            ]
        },
        "comments": {
            "patterns": [
                {
                    "name": "comment.line.double-slash.fsk",
                    "match": "//.*"
                },
                {
                    "name": "comment.block.fsk",
                    "begin": "/\\*",
                    "end": "\\*/"
                }
            ]
        },
        "numbers": {
            "patterns": [
                {
                    "name": "constant.numeric.fsk",
                    "match": "\\b\\d+(\\.\\d+)?\\b"
                }
            ]
        }
    },
    "scopeName": "source.fsk"
}