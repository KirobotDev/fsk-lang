class MathLib {
    fn sin(x) { return Math.sin(x); }
    fn cos(x) { return Math.cos(x); }
    fn sqrt(x) { return Math.sqrt(x); }
    fn abs(x) { return Math.abs(x); }
    fn pow(x, y) { return Math.pow(x, y); }
    
    fn floor(x) {
        return x - (x % 1);
    }

    fn ceil(x) {
        let f = this.floor(x);
        if (f < x) return f + 1;
        return f;
    }
}

let math = new MathLib();
math.PI = Math.PI;
math.E = Math.E;
